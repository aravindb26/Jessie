{
  "compilerOptions": {
    "baseUrl": ".",
    "moduleResolution": "node",
    "target": "esnext",
    "module": "CommonJS",
    "noImplicitAny": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "alwaysStrict": true,
    "downlevelIteration": true
  },
  "include": ["lib/**/*.js"],
  "exclude": ["node_modules/**"]
}
