{
  "name": "@jessie.js/eslint-plugin",
  "version": "0.3.0",
  "description": "Jessie-specific ESLint plugin",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Agoric",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha test/test-*.js",
    "build": "exit 0",
    "lint-fix": "eslint '**/*.js' --fix",
    "lint": "eslint '**/*.js'"
  },
  "dependencies": {
    "requireindex": "~1.1.0"
  },
  "devDependencies": {
    "eslint": "^7.23.0",
    "mocha": "^10.2.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": [
      "plugin:@jessie.js/internal"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  },
  "license": "Apache-2.0"
}
