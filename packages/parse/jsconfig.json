{
    "compilerOptions": {
        "baseUrl": ".",
        "moduleResolution": "node",
        "target": "esnext",
        "module": "es6",
        "typeRoots": ["../types", "./node_modules/@types"],
        "noImplicitAny": true,
        "noEmit": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "alwaysStrict": true,
        "downlevelIteration": true,
        "types": ["@jessie.js/types/lib.jessie"]
    },
    "include": [
        "src/**/*.js"
    ],
    "exclude": [
        "node_modules/**"
    ]
}
